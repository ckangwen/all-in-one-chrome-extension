{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)|^(react-native(.*)$)",
    "^(next/(.*)$)|^(next$)",
    "^(vue/(.*)$)|^(vue$)",
    "<THIRD_PARTY_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^@(.*)/(.*)$",
    "",
    "^~/(.*)$",
    "",
    "^@/(.*)$",
    "",
    "^[./]",
    "",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ]
}
